        WriteConfig "是否已推关2"&账号信息, False, True
        //重置接皇榜状态
        WriteConfig "是否已接皇榜"&账号信息, False, True
        //重置签到
        WriteConfig "是否已签到"&账号信息, False, True
        //重置铸币
        WriteConfig "今日是否铸币"&账号信息, False, True
        //重置寻访次数
        WriteConfig "今日寻访剩余次数"&账号信息, 5, True
        //重置补星状态
        WriteConfig "今日已补星1"&账号信息, False, True
        WriteConfig "今日已补星2"&账号信息, False, True
        //重置名将副本状态
        WriteConfig "今日已打名将"&账号信息, False, True
        //重置商城购买状态
        WriteConfig "今日商城已购买" & 账号信息, False, True
        //重置VIP等级
        WriteConfig "VIP等级"&账号信息, 99, True
    End If
End Sub

Sub 处理渠道图标()
    Dim intX, intY
    FindColor 365, 57, 374, 67, "FFFFFF", 0, 0.99, intX, intY
    If intX > -1 And intY > -1 Then 
        TouchDown intX, intY
        TouchMove 0, 0
        TouchUp 
        Tap 0, 0
        Tap 0, 0
    End If
End Sub

Sub 账号信息处理(编号)
    Rem 获取账号信息
    If 编号 > 5 Then 
        登录成功次数 = 1
        编号 = 1
    End If
	
    账号信息 = ReadUIConfig("账号" & 编号)
    WriteConfig "账号信息", 账号信息, True
    账号密码 = ReadUIConfig("密码" & 编号)
    WriteConfig "账号密码", 账号密码, True
    区服 = ReadUIConfig("区服" & 编号)
    WriteConfig "区服", 区服, True
    选服重试次数 = Int(ReadUIConfig("选服重试次数" & 编号, 10))
    WriteConfig "选服重试次数", 选服重试次数, True
    If 账号信息 = "" Or 账号密码 = "" Then 
        TracePrint "账号信息有误 重新获取"
        登录成功次数 = 登录成功次数 + 1 : 编号 = 编号 + 1
        Goto 获取账号信息
    End If
End Sub

Sub 回登录界面()
    Call 进出界面.回主城(1)
    容错参数 = 0
    Rem 进设置界面
    Touch 624, 1233, 100
    Delay 1500
    If 场景相关操作.进入场景是否正确(60) Then 
        TracePrint "进入设置界面"
        Rem 退出游戏
        Touch 225, 745, 100
        Delay 5000
        If 场景相关操作.进入场景是否正确(21) Then 
            TracePrint "成功退出游戏"
            来路是否是切换账号 = True
            场景相关操作.通用界面关闭
            Delay 1500
            Touch 110, 140, 100
            Delay 1500
        ElseIf 容错参数 >= 5 Then
            ShowMessage "无法正常退出", 5000, 50, 620
            Delay 5000
        ElseIf 容错参数 < 5 Then
            Goto 退出游戏
        End If
    ElseIf 容错参数 < 5 Then
        容错参数 = 容错参数 + 1
        Delay 2000
        Goto 进设置界面
    ElseIf 容错参数 >= 5 Then
        ShowMessage "无法正常退出,请确认设置按钮是否被遮挡了", 5000, 50, 620
        Delay 5000
    End If
End Sub

Function 体力判断
    If 当前体力 = Null Then 
        TracePrint "当前体力数据异常,强制设置为0"
        ShowMessage "当前体力数据异常,强制设置为0", 2000, 50, 620
        Delay 2000
        当前体力 = 0
    End If
    If 当前体力 < 体力阀值 Then 
        体力判断 = False
    Else 
        体力判断 = True
    End If
End Function

Sub 错误记录(记录内容)
    Dim 写入行数, 文件判断结果
    文件判断结果 = Dir.Exist("localDataDir/log.txt")
    If 文件判断结果 = 1 Then 
        写入行数 = File.LinesNumber("localDataDir/log.txt") + 1
        Call File.WriteLine("localDataDir/log.txt", 写入行数, 记录内容)
        TracePrint "追加记录"
    ElseIf 文件判断结果 Then
        Call File.Write("localDataDir/log.txt", 记录内容)
        TracePrint "新建记录"
    End If
End Sub

Sub 定位主城坐标()
    ShowMessage "定位主城坐标", 1000, 50, 620
    野外主城坐标X = Int(ReadUIConfig("主城X坐标")) : 野外主城坐标Y = Int(ReadUIConfig("主城Y坐标"))
    If 野外主城坐标X <> 0 And 野外主城坐标Y <> 0 Then 
        TracePrint "用户自己设置了主城坐标,不用定位了"
        Exit Sub 
    End If
    
    Rem 定位
    Call 进出界面.回主城(1)
    //去野外
    If 进出界面.城内进固定界面(2, 相关数据解析.去野外坐标X, 相关数据解析.去野外坐标Y, 0, False) Then 
        //        Delay 2000
        容错参数 = 0
        Rem 定位坐标
        Dim ocrchar1, ocrchar2, tempStr
        ocrchar1 = 类型转换.SuperOcr(678, 597, 706, 648, "BFF3FB-111111|AFDCE2-111111|8FB0B3-111111|7F9A9C-111111|9FC6CB-111111",0.9)
        野外主城坐标X = Int(ocrchar1)
        ocrchar2 = 类型转换.SuperOcr(678, 712, 706, 765, "BFF3FB-111111|AFDCE2-111111|8FB0B3-111111|7F9A9C-111111|9FC6CB-111111",0.9)
        野外主城坐标Y = Int(ocrchar2)
        TracePrint ocrchar1, ocrchar2
        tempStr = "主城坐标" & 野外主城坐标X & "," & 野外主城坐标Y
        TracePrint tempStr
        If (野外主城坐标X = 0 Or 野外主城坐标Y = 0 Or 野外主城坐标X = Null Or 野外主城坐标Y = Null ) And 容错参数 < 5 Then 
            容错参数 = 容错参数 + 1
            TracePrint "坐标定位有问题,重试"
            Goto 定位坐标
        End If
        ShowMessage tempStr, 2000, 50, 620
        Delay 2000
        ShowMessage "如果主城坐标识别不正确,请停止运行脚本,在设置中手动输入", 1000, 50, 620
        Delay 1000
    Else 
        Goto 定位
    End If
    
End Sub

