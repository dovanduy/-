                            Delay 2000
                            Exit Do
                        End If
                    End If
                Next
            Else 
                TracePrint "进入界面失败,放弃"
                ShowMessage "进入界面失败,放弃", 2000, 50, 620
                Delay 2000
            End If
        Else 
