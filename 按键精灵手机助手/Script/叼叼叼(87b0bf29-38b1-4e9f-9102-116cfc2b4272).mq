Function 判断队伍子状态()
    Rem 确认状态
    Dim ocrchar1, ocrchar2, ocrchar3
    ocrchar1=类型转换.SuperOcr(122,1047,140,1091,"E4E4E4-111111|CBCBCA-111111|A1A1A0-111111",0.9)
    Traceprint ocrchar1
    ocrchar2=类型转换.SuperOcr(250,1046,268,1091,"E4E4E4-111111|CBCBCA-111111|A1A1A0-111111",0.9)
    Traceprint ocrchar2
    ocrchar3=类型转换.SuperOcr(290,1046,313,1091,"E4E4E4-111111|CBCBCA-111111|A1A1A0-111111",0.9)
    Traceprint ocrchar3
    If ocrchar1 = "驻防" Then 
        TracePrint "部队空闲中"
        判断队伍子状态 = 1
    ElseIf ocrchar2 = "撤防" Then
        TracePrint "部队驻防中"
        判断队伍子状态 = 2
    ElseIf ocrchar2 = "撤回" Then
        TracePrint "部队在外驻扎中"
        判断队伍子状态 = 3
    ElseIf ocrchar2 = "加速" Then
        TracePrint "部队返回中"
        判断队伍子状态 = 4
    ElseIf ocrchar3 = "撤回" Then
        TracePrint "部队出发中"
        判断队伍子状态 = 5
    End If
End Function

Function 被攻击目标判断(ocrchar)
    //目标枚举: 1 = 主城, 2 = 矿
    If ocrchar = "正在" Then 
        TracePrint "矿被打了"
        被攻击目标判断 = 2
    ElseIf ocrchar = "主城"
        TracePrint "主城被打"
        被攻击目标判断 = 1
    Else 
        TracePrint "还未知的类型"
        SnapShot "/sdcard/test.png",0,0,720,1280
    End If
	
End Function

