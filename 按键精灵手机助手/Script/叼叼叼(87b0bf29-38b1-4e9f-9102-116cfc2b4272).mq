    //判断粮食是否爆仓
    FindColor 680,640,715,762,"4D4DF3",0,0.9,intX,intY
    If intX > -1 And intY > -1 Then
        TracePrint "粮食爆仓"
        确认爆仓资源 = 0
    End If
