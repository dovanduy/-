Sub 商城购买()
    Dim 是否进行商城购买, 商城购买次数, 已购买次数
    是否进行商城购买 = ReadUIConfig("是否进行商城购买") : 商城购买次数 = Int(ReadUIConfig("商城购买次数")) : 已购买次数 = ReadConfig("商城已购买次数", 0)
    TracePrint "是否进行商城购买",是否进行商城购买,"商城购买次数",商城购买次数,"已购买次数",已购买次数
    If 是否进行商城购买 = True Then 
        If 商城购买次数 > 已购买次数 Then 
            Dim 字符长度, 准备购买次数, 可购买次数, temp, intX, intY, ocrchar
            Rem 进商城界面
            所在场景 = 场景相关操作.所在场景()
            If 所在场景 = 1 Or 所在场景 = 2 Then 
                Touch 商城坐标X, 商城坐标Y, 100
                Delay 1500
                Goto 进商城界面
            ElseIf 所在场景 = 4 Then
                TracePrint "已经在商城界面"
            Else 
                Call 回主城(1)
                Goto 进商城界面
            End If
            Delay 1500
            //购买包子
            FindPic 413,421,575,605,"Attachment:已售罄.png","000000",0,0.9,intX,intY
            If intX > -1 And intY > -1 Then
                TracePrint "包子已经卖光,无法购买"
            Else 
                容错参数 = 0
                Rem 点击购买1
                Touch 450, 500, 100
                Delay 1000
                FindPic 167,536,243,732,"Attachment:商城购买.png","000000",0,0.95,intX,intY
                If intX > -1 And intY > -1 Then
                    TracePrint "已经打开数量界面"
                    容错参数 = 0
                    Rem 设置次数1
                    ocrchar=Ocr(287,786,328,909,"BFD5E2-111111|8897A0-111111|AEC2CE-111111|9AACB6-111111|ABBECA-111111|9BADB7-111111",0.9)
                    Traceprint ocrchar
                    字符长度 = Len(ocrchar)
                    temp = InStr(1, ocrchar, "/")
                    准备购买次数 = Int(Left(ocrchar, temp - 1))
                    可购买次数 = Int(Right(ocrchar, 字符长度 - temp))
