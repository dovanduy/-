Sub 判断入侵()
    Dim 放弃抵抗,自动撤矿,回防主城
    放弃抵抗 = ReadUIConfig("防守策略",False) : 自动撤矿 = ReadUIConfig("自动撤矿", True) : 回防主城 = ReadUIConfig("回防主城", False)
    If 放弃抵抗 = True Then 
        TracePrint "放弃抵抗"
        Exit Sub
    End If
    Rem 识别敌情
    TracePrint "开始判断敌人情况"
    Call 进出界面.回主城(1)
    Dim intX,intY,被攻击目标,ocrchar
    FindPic 359,546,676,1119,"Attachment:兵临城下.png","000000",0,0.9,intX,intY
    If intX > -1 And intY > -1 Then
        TracePrint "有敌情"
        Touch intX, intY, 100
        Delay 1500
        //第一个信息
        ocrchar = 类型转换.SuperOcr(530, 330, 565, 385, "4B4BF0-111111|4444D7-111111|3B3CB9-111111|2D2D87-111111|272B63-111111", 0.7)
        Traceprint ocrchar
        被攻击目标 = 被攻击目标判断(ocrchar)
        If 被攻击目标 = 2 And 自动撤矿 Then 
            ocrchar=类型转换.SuperOcr(530,561,559,637,"4B4BF0-111111|4444D7-111111|3B3CB9-111111|2D2D87-111111|272B63-111111",0.7)
            Traceprint ocrchar
            
            FindStr(530,561,559,637,",","4B4BF0-111111|4444D7-111111|3B3CB9-111111|2D2D87-111111|272B63-111111",0.8,intX,intY)
            If intX > -1 And intY > -1 Then
                Touch intX, intY, 100
                Delay 5000
                Call 撤矿()
                Goto 识别敌情
            End If
            Call 队伍撤防()
        ElseIf 被攻击目标 = 1 Then
            Call 主城防守(回防主城)
            
        End If
        //第二个信息
        ocrchar=类型转换.SuperOcr(317,332,353,386,"4B4BF0-111111|4444D7-111111|3B3CB9-111111|2D2D87-111111|272B63-111111",0.7)
        Traceprint ocrchar
        被攻击目标 = 被攻击目标判断(ocrchar)
        If 被攻击目标 = 2 Then 
            ocrchar = 类型转换.SuperOcr(321, 561, 351, 637, "4B4BF0-111111|4444D7-111111|3B3CB9-111111|2D2D87-111111|272B63-111111", 0.7)
            Traceprint ocrchar
            FindStr(321,561,351,637,",","4B4BF0-111111|4444D7-111111|3B3CB9-111111|2D2D87-111111|272B63-111111",0.8,intX,intY)
            If intX > -1 And intY > -1 Then
                Touch intX, intY, 100
                Delay 1000
                Call 撤矿()
                Goto 识别敌情
            End If
            Call 队伍撤防()
