Sub 切换自动补兵(类型)
    //类型说明:1 = 自动补,2 = 取消自动补
    Dim intX,intY
    容错参数 = 0
    Do Until 容错参数 >= 3
        Select Case 类型
        Case 1            
            FindPic 65,1221,112,1267,"Attachment:兵力勾.png","000000",0,0.9,intX,intY
            If intX > -1 And intY > -1 Then
                TracePrint "选中了自动补兵"
                Exit Sub
            Else 
                Touch 80, 1240, 100
                容错参数 = 容错参数 + 1
                Delay 1000
            End If
        Case 2
            FindPic 65,1221,112,1267,"Attachment:兵力勾.png","000000",0,0.9,intX,intY
            If intX > -1 And intY > -1 Then 
                容错参数 = 容错参数 + 1
                Touch 80, 1240, 100
                Delay 1000
            Else 
                TracePrint "没有选中自动补兵"
                Exit Sub
            End If
        End Select
    Loop
	
End Sub

Function 单个队伍满员率()
    Dim 判断区间起X1, 判断区间起Y1, 判断区间止X2, 判断区间止Y2, 变化差值X, 变化差值Y, 判断区间X1, 判断区间X2, 判断区间Y1, 判断区间Y2, 满员率
    Dim temp1, temp2, temp3, ocrchar, intX,intY
    判断区间起X1 = 410 : 判断区间起Y1 = 746 : 判断区间止X2 = 430 : 判断区间止Y2 = 832 : 变化差值X = 159 : 变化差值Y = 151 : 满员率 = 0 : temp3 = 6
    For i = 0 To 5
        判断区间X1 = 判断区间起X1 - Int(i / 2) * 变化差值X
        判断区间X2 = 判断区间止X2 - Int(i / 2) * 变化差值X
        判断区间Y1 = 判断区间起Y1 + (i Mod 2) * 变化差值Y
        判断区间Y2 = 判断区间止Y2 + (i Mod 2) * 变化差值Y
        
        TracePrint 判断区间X1, 判断区间X2, 判断区间Y1, 判断区间Y2
        ocrchar=Ocr(判断区间X1,判断区间Y1,判断区间X2,判断区间Y2,"C2B243-111111|636466-111111|9B9FA5-111111|7B7F82-111111|BFC4CB-111111",0.7)
        Traceprint ocrchar
        If UTF8.InStr(1, ocrchar, "空") > 0 Then 
            TracePrint i&"号位为空"
            temp3 = temp3 - 1
        Else 
            FindColor 判断区间X1,判断区间Y1,判断区间X2,判断区间Y2,"C2B243",2,1,intX,intY
            If intX > -1 And intY > -1 Then
                temp1 = intY - 750
                If temp1 >= 150 Then 
                    temp1 = intY - 899
                End If
                temp2 = 20 + temp1
                If temp2 > 100 Then 
                    temp2 = 100
                End If
                满员率 = 满员率 + temp2
                TracePrint i&"号位满员率"&temp2
            Else 
                FindColor 判断区间X1, 判断区间Y1, 判断区间X2, 判断区间Y2, "C1B143", 2, 1, intX, intY
                If intX > -1 And intY > -1 Then
                    temp1 = intY - 750
                    If temp1 >= 150 Then 
                        temp1 = intY - 899
                    End If
                    temp2 = 20 + temp1
                    If temp2 > 100 Then 
                        temp2 = 100
                    End If
                    满员率 = 满员率 + temp2
                    TracePrint i&"号位满员率"&temp2
                Else 
                    FindColor 判断区间X1, 判断区间Y1, 判断区间X2, 判断区间Y2, "C1B042", 2, 1, intX, intY
                    If intX > -1 And intY > -1 Then
                        temp1 = intY - 750
                        If temp1 >= 150 Then 
                            temp1 = intY - 899
                        End If
                        temp2 = 20 + temp1
                        If temp2 > 100 Then 
                            temp2 = 100
                        End If
                        满员率 = 满员率 + temp2
                        TracePrint i&"号位满员率"&temp2
                    End If
                End If
            End If
        End If  
    Next
    单个队伍满员率 = 满员率 / temp3
    TracePrint "整体满员率"&满员率
End Function
//加载队伍出征设置
Function 队伍设置()
    Dim 打怪队伍, 采集队伍, 寻访队伍, 打怪种类, 打怪等级, 采集类型, 矿坑等级
    打怪队伍 = Array() : 采集队伍 = Array() : 寻访队伍 = Array() : 打怪种类 = Array() : 打怪等级 = Array() : 采集类型 = Array() : 矿坑等级 = Array()
    //打怪队伍
    For i = 0 To 3
        打怪队伍(i) = ReadUIConfig("队伍" & i)
    Next
    //采集队伍
    For i = 0 To 3
        采集队伍(i) = ReadUIConfig("采集队伍" & i)
    Next
    //寻访队伍
    For i = 0 To 3
        寻访队伍(i) = ReadUIConfig("寻访队伍" & i)
    Next

    //怪物种类枚举: 0 = 黄巾军, 1 = 流寇, 2 = 山贼, 4 = 马贼
    For i = 0 To 3
        打怪种类(i) = ReadUIConfig("怪物类型" & i)
        //        TracePrint "打怪种类" & i, 打怪种类(i)
    Next
    //怪物等级枚举:普通1, 普通2 ... 普通10, 精英1,...精英5
    For i = 0 To 14
        打怪等级(i) = ReadUIConfig("普通" & i)
    Next

    //采集种类枚举: 0 = 农田, 1 = 伐木场, 2 = 采石场, 3 = 冶炼厂
    For i = 0 To 3
        采集类型(i) = ReadUIConfig("采集类型" & i)
        //        TracePrint "采集类型" & i, 采集类型(i)
    Next
    //矿坑等级: 矿坑等级1.....矿坑等级10
    For i = 0 To 9
        矿坑等级(i) = ReadUIConfig("矿坑等级" & i)
    Next
    
    队伍设置 = Array(打怪队伍, 采集队伍, 寻访队伍, 打怪种类, 打怪等级, 采集类型, 矿坑等级 )
End Function

Sub 派遣队伍()
    //类型一: 1 = 普通刷怪, 2 = 皇榜任务
    ShowMessage "派遣队伍", 1000, 50, 620
    Dim 用户设置, 打怪队伍, 队伍状态, 伤兵信息, 是否使用行军令, 打怪种类, 打怪等级, 打怪次数, 满员率设置, 队伍兵量信息, 采集队伍, 采集类型, 矿坑等级, 寻访队伍

    TracePrint "准备开始派遣队伍"
    用户设置 = 队伍设置()
    伤兵信息 = 兵事处理()
    队伍状态 = 判断队伍状态()
    打怪队伍 = 用户设置(0)
    采集队伍 = 用户设置(1)
    寻访队伍 = 用户设置(2)
    打怪种类 = 用户设置(3)
    打怪等级 = 用户设置(4)
    采集类型 = 用户设置(5)
    矿坑等级 = 用户设置(6)
    打怪次数 = Int(ReadUIConfig("打怪次数", 0))
    是否使用行军令 = ReadUIConfig("是否使用行军令", False)
    满员率设置 = Int(ReadUIConfig("满员率", 100))
    If 打怪次数 > 0 Then 
        For i = 0 To 3
            If 打怪队伍(i) Then 
                If 队伍状态(i) = 1 Then 
                    If (伤兵信息(0) / 伤兵信息(1) * 100) < Int(ReadUIConfig("伤兵比例")) Then 
                        队伍兵量信息 = 判断队伍兵量(i)
                        If 队伍兵量信息 > 满员率设置 Then 
                            If 打怪次数 > 派遣出征次数 Then 
                                TracePrint "出城寻找怪物"
                                Call 搜索目标(1, 打怪种类, 打怪等级)
                                Call 前往目标(1, i)
                                队伍状态(i) = 0
                            End If
                        End If
                    End If
                End If	
            End If
        Next
    End If
    
    If ReadUIConfig("自动识别矿等级") Then 
        TracePrint "开启了自动识别等级"
        矿坑等级 = Array()
        矿坑等级(识别矿坑等级() - 1) = True
    End If
    For i = 0 To 3
        If 采集队伍(i) Then 
            If 队伍状态(i) = 1 Then 
                队伍兵量信息 = 判断队伍兵量(i)
                If 队伍兵量信息 > 满员率设置 Then 
                    If ReadUIConfig("智能采集", True) Then 
                        采集类型 = Array()
                        采集类型(最少资源类型()) = True
                        For Each a In 采集类型
                            TracePrint a
                        Next
                    End If
                    TracePrint "出城寻找资源"
                    Call 搜索目标(2, 采集类型, 矿坑等级)
                    Call 前往目标(2, i)
                    队伍状态(i) = 0
                End If
            End If	
        End If
    Next
    If ReadConfig("今日寻访剩余次数"&账号信息) = 0 Then 
        TracePrint "今日寻访次数为0"
    Else 
        For i = 0 To 3
            If 寻访队伍(i) Then 
                If 队伍状态(i) = 1 Then 
                    TracePrint "出城寻访"
                    //去野外
                    进出界面.城内进固定界面(2, 相关数据解析.去野外坐标X, 相关数据解析.去野外坐标Y, 0, False)
                    Dim 寻访坐标X, 寻访坐标Y
                    寻访坐标X = Int(ReadUIConfig("寻访城池X坐标"&登录成功次数)) : 寻访坐标Y = Int(ReadUIConfig("寻访城池Y坐标"&登录成功次数))
                    Call 移动大地图(寻访坐标X, 寻访坐标Y)
                
                    容错参数 = 0
                    Rem 点击城池
                    Call 进出界面.界面收起展开(3)
                    Touch 380, 640, 100
                    Delay 1000
                    Dim intX,intY
                    FindPic 185,666,617,1019,"Attachment:寻访图标.png","000000",0,0.7,intX,intY
                    If intX > -1 And intY > -1 Then
                        TracePrint "点击城池成功"
                        容错参数 = 0
                        Rem 点击寻访图标
                        Touch intX + 5, intY + 5, 100
                        Delay 1000
                        If 场景相关操作.进入场景是否正确(48) Then 
                            TracePrint "进入寻访界面"
                            容错参数 = 0
                            Rem 识别剩余次数
                            Dim ocrchar
                            ocrchar = 类型转换.SuperOcr(493,578,517,619,"489867-111111|5BC182-111111|3E7F57-111111",0.9)
                            TracePrint ocrchar
                            If Len(ocrchar) = 3 Then 
                                If Int(Left(ocrchar, 1)) = 0 Then 
                                    TracePrint "今日寻访次数已用完"
                                    WriteConfig "今日寻访剩余次数"&账号信息, 0, True
                                Else 
                                    Call 前往目标(3, i)
                                    队伍状态(i) = 0
                                End If
                            ElseIf 容错参数 < 5 Then
                                容错参数 = 容错参数 + 1
                                TracePrint "识别错误,重试"
                                SnapShot "/sdcard/test1.png",0,0,720,1280
                                Goto 识别剩余次数
                            End If
                        ElseIf 容错参数 < 5 Then
                            TracePrint "进入寻访界面失败,重试"
                            容错参数 = 容错参数 + 1
                            Goto 点击寻访图标
                        End If
                    ElseIf 容错参数 < 5 Then
                        容错参数 = 容错参数 + 1
                        TracePrint "点击城池失败,重试"
                        Goto 点击城池
                    End If
                End If	
            End If
        Next
    End If
    TracePrint "派遣循环完毕"
End Sub

