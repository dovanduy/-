    If ReadConfig("今日商城已购买"&账号信息) = 1 Then 
        TracePrint "今日商城已购买,放弃"
        Exit Sub
    End If
    If 是否进行商城购买 Then 
        容错参数 = 0
        //进商城界面
        进出界面.城内外通用进界面(4, 相关数据解析.商城坐标X, 相关数据解析.商城坐标Y, 0)
