                                        Delay 2000
                                    End If 
                                End If
                            Else 
                                TracePrint "副本剩余次数为0,重新选择"
                            End If
                        Else 
                            TracePrint "进入界面失败,放弃"
                            ShowMessage "进入界面失败,放弃", 2000, 50, 620
                            Delay 2000
                        End If
                    Else 
                        FindPic 起始X1,起始Y1,起始X2,起始Y2,"Attachment:升阶升星不可前往.png","000000",2,0.9,intX,intY
                        If intX > -1 And intY > -1 Then
