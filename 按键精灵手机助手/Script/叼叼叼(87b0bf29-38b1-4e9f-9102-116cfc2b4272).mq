Sub 祭祀等待(类型)
    TracePrint UseDict(1)
    Dim ocrchar
    Rem 识别文字
    ocrchar = Ocr(373,956,535,1017,"F4FFFF-111111|FDFFFF-111111|D6DEEA-111111",0.9)
    Traceprint ocrchar
    祭祀切换 = False
    Select Case 类型
    Case 1
        If UTF8.InStr(1, ocrchar, "农") > 0 Then
            祭祀切换 = True
            Exit Function
        End If
    Case 2
        If UTF8.InStr(1, ocrchar, "木") > 0 Then 
            祭祀切换 = True
            Exit Function
        End If
    Case 3
        If UTF8.InStr(1, ocrchar, "石") > 0 Then 
            祭祀切换 = True
            Exit Function
        End If
    Case 4
        If UTF8.InStr(1, ocrchar, "矿") > 0 Then 
            祭祀切换 = True
            Exit Function
        End If
    End Select
    Delay 1000
		
End Sub

