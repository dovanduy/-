Sub 主城防守(回防主城)
    If ReadUIConfig("自动免战", True) Then 
        TracePrint "设置了自动挂免战"
        //自动挂免流程
        容错参数 = 0
        Dim 无元宝
        Rem 挂免战
        Call 进出界面.回主城(1)
        If 进出界面.进无坐标子界面(79, 1, 400, 560, 670, 1100, "Attachment:加buff1.png", 0.9) And 容错参数 < 5 And Not 无元宝 Then 
            Dim ocrchar, 时,分,秒,总剩余秒数
            ocrchar=类型转换.SuperOcr(460,427,490,538,"59A678-111111|4C8D66-111111",0.9)
            Traceprint ocrchar
            If ocrchar <> Null Then 
                TracePrint "当前在免战中"
                时 = Int(Left(ocrchar, 2))
                分 = Int(Mid(ocrchar, 3, 2))
                秒 = Int(Right(ocrchar, 2))
                总剩余秒数 = 时 * 3600 + 分 * 60 + 秒
                If 总剩余秒数 > 1800 Then 
                    TracePrint "剩余时间还多,暂时不重新挂免战"
                    Exit Function
                End If  
            End If
            Touch 430, 560, 100
            Delay 1500
            ocrchar = 类型转换.SuperOcr(231,727,269,796,"B3B6B4-111111|CCCDCC-111111|DCDCDC-111111|E9E9E9-111111|7D837F-111111",1)
            TracePrint ocrchar
            If ocrchar = "确定" Then 
                TracePrint "没用免战牌了,用元宝买"
                Touch 250, 760, 100
                Delay 3500
            ElseIf ocrchar = "充值" Then
                TracePrint "元宝也不够了,无法免战了"
                场景相关操作.通用界面关闭 
                Delay 2000
                无元宝 = True
            End If
            容错参数 = 容错参数 + 1
            Goto 挂免战
        End If
    End If
    容错参数 = 0
    Dim 队伍子状态,驻防容错次数,是否已加速
    驻防容错次数 = 8
    Rem 防守
    进出界面.城内外通用进界面(8, 相关数据解析.出征坐标X, 相关数据解析.出征坐标Y, 0)
    队伍子状态 = 判断队伍子状态() 
    If 队伍子状态 = 2 Then 
        TracePrint "等待敌人来犯"
        部队驻防时间 = Time()
    ElseIf 队伍子状态 = 1 And 容错参数 < 驻防容错次数 Then
        If 场景相关操作.进入场景是否正确(8) Then 
            TracePrint "补兵"
            Touch 500, 1065, 100
            Delay 2000
            切换自动补兵 (2)
            Touch 85, 600, 100
            Delay 1000
            场景相关操作.通用界面关闭 
            Delay 1000 
            TracePrint "点击驻防"
            Touch 155, 1065, 100
            Delay 1000
            容错参数 = 容错参数 + 1
            Goto 防守
        ElseIf 容错参数 < 5 Then
            进出界面.城内外通用进界面 8, 相关数据解析.出征坐标X, 相关数据解析.出征坐标Y, 0
            容错参数 = 容错参数 + 1
            Goto 防守
        Else 
            TracePrint "进队伍界面错误次数太多,放弃"
        End If
    ElseIf 队伍子状态 = 3 And 回防主城 And 容错参数 < 驻防容错次数 Then
        TracePrint "撤回部队"
        Touch 500, 1065, 100
        Delay 1000
        Touch 245, 760, 100
        Delay 1000
        是否已加速 = True
        If Not 是否已加速 Then 
            Dim 加速次数
            If VIP等级 = Null Then 
                TracePrint "VIP等级未识别,跳过加速"
            Else 
                If VIP等级 = 0 Then 
                    加速次数 = 1
                ElseIf VIP等级 <= 3 Then
                    加速次数 = 2
                ElseIf VIP等级 <= 6 Then
                    加速次数 = 3
                ElseIf VIP等级 <= 9 Then
                    加速次数 = 4
                ElseIf VIP等级 <= 15 Then
                    加速次数 = 5
                End If
                TracePrint "点击加速"
                Touch 290, 1065, 100
                Delay 1000
                For 加速次数
                    Touch 225, 635, 100
                    Delay 500
                Next
                是否已加速 = True
                Do Until 场景相关操作.进入场景是否正确(8) Or 容错参数 > 5
                    场景相关操作.通用界面关闭 
                    Delay 1500
                Loop
            End If
        End If
        Goto 防守
    ElseIf 队伍子状态 = 4 Then
        TracePrint "等待部队返回"
        Delay 2000
        Goto 防守
    ElseIf 队伍子状态 = 5 And 回防主城 And 容错参数 < 驻防容错次数 Then
        TracePrint "阻止部队行进"
        Touch 330, 1065, 100
        Delay 1000
        Touch 225, 635, 100
        Delay 1000
        Do Until 场景相关操作.进入场景是否正确(8) Or 容错参数 > 5
            场景相关操作.通用界面关闭 
            Delay 1500
        Loop
        Goto 防守
    ElseIf 容错参数 >= 驻防容错次数 Then
        TracePrint "确认驻防错误次数太多,放弃"
    End If
End Sub

Sub 队伍撤防()
    容错参数 = 0
    If 部队驻防时间 = Null Or (Time() - 部队驻防时间 > 300) Then 
        Dim 队伍子状态
        Rem 撤防
        进出界面.城内外通用进界面(8, 相关数据解析.出征坐标X, 相关数据解析.出征坐标Y, 0)
        队伍子状态 = 判断队伍子状态()
        If 队伍子状态 = 2 And 容错参数 < 5 Then 
            TracePrint "撤防"
            Touch 285, 1065, 100
            Delay 1000
            Goto 撤防
        ElseIf 容错参数 < 5 Then
            TracePrint "队伍没有在驻防中"
        ElseIf 容错参数 >= 5 Then
            TracePrint "撤防失败"
        End If
    End If
    TracePrint "撤防结束"
End Sub

