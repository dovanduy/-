Sub 搜索目标(寻找类型, 目标种类, 目标等级)
    //寻找类型,1 = 怪, 2 = 资源
    Dim 坐标增量, 新坐标X, 新坐标Y, 目标坐标信息
    目标坐标信息 = ReadConfig("目标坐标信息" & 账号信息 & 寻找类型)
    
    Call 进附近界面(寻找类型)
    //进筛选界面
    进出界面.进入子界面(46, 47, 相关数据解析.筛选界面坐标X, 相关数据解析.筛选界面坐标Y)
    Call 筛选设置(寻找类型, 目标种类, 目标等级)
    
    //目标坐标信息是为了搜索怪物从上次找到目标的地方继续,避免每次都从主城开始,浪费时间
    
    If 确认目标(寻找类型, 目标种类, 目标等级) Then 
        目标坐标信息 = Array(野外主城坐标X, 野外主城坐标Y, 1, 1, 1, 1, 1, Time())
        Exit Sub
    End If
    
    //考虑到搜索资源时不需要,但是目前已经耦合到一起了,所以在最开始的时候加一个判断
    If 目标坐标信息 = Null Then 
        目标坐标信息 = Array(野外主城坐标X, 野外主城坐标Y, 1, 1, 1, 1, 1, Time())
    ElseIf (Time() - 目标坐标信息(7)) >= 3600 Or 寻找类型 = 2 Then
        TracePrint "目标坐标信息",目标坐标信息(0),目标坐标信息(1),目标坐标信息(7)
        目标坐标信息 = Array(野外主城坐标X, 野外主城坐标Y, 1, 1, 1, 1, 1, Time())
    End If
    WriteConfig "目标坐标信息" & 账号信息 & 寻找类型, 目标坐标信息, True
    
    For Each a In 目标坐标信息
        If a = Null Then 
            a = 1	
        End If
    Next
    
    
    新坐标X = Int(目标坐标信息(0)) : 新坐标Y = Int(目标坐标信息(1)) : 坐标增量 = 15
    Dim a, b, c, d
    For i = 目标坐标信息(2) To 50
        TracePrint "开始第"&i&"轮找怪"
        新坐标X = 新坐标X - 坐标增量
        新坐标Y = 新坐标Y - 坐标增量
        For a = 目标坐标信息(3) To i * 2
            TracePrint "下侧第"&a&"次找怪"
            新坐标X = 新坐标X + 坐标增量
            If 确认坐标有效性(新坐标X, 新坐标Y) Then 
                Call 移动大地图(新坐标X, 新坐标Y)
                If 确认目标(寻找类型, 目标种类, 目标等级) Then
                    目标坐标信息(0) = 新坐标X : 目标坐标信息(1) = 新坐标Y : 目标坐标信息(2) = i : 目标坐标信息(3) = a
                    WriteConfig "目标坐标信息"&账号信息&寻找类型, 目标坐标信息, True
                    Exit Sub
                End If
            End If
        Next
        a = 1
        For b = 目标坐标信息(4) To i * 2
            TracePrint "右侧第"&b&"次找怪"
            新坐标Y = 新坐标Y + 坐标增量
            If 确认坐标有效性(新坐标X, 新坐标Y) Then 
                Call 移动大地图(新坐标X, 新坐标Y)
                If 确认目标(寻找类型, 目标种类, 目标等级) Then 
                    目标坐标信息(0) = 新坐标X : 目标坐标信息(1) = 新坐标Y : 目标坐标信息(2) = i : 目标坐标信息(4) = b
                    WriteConfig "目标坐标信息"&账号信息&寻找类型, 目标坐标信息, True
                    Exit Sub
                End If
            End If
        Next
        b = 1
        For c = 目标坐标信息(5) To i * 2
            TracePrint "上侧第"&c&"次找怪"
            新坐标X = 新坐标X - 坐标增量
            If 确认坐标有效性(新坐标X, 新坐标Y) Then 
                Call 移动大地图(新坐标X, 新坐标Y)
                If 确认目标(寻找类型, 目标种类, 目标等级) Then 
                    目标坐标信息(0) = 新坐标X : 目标坐标信息(1) = 新坐标Y : 目标坐标信息(2) = i : 目标坐标信息(5) = c
                    WriteConfig "目标坐标信息"&账号信息&寻找类型, 目标坐标信息, True
                    Exit Sub
                End If
            End If
        Next
        c = 1
        For d = 目标坐标信息(6) To i * 2
            TracePrint "左侧第"&d&"次找怪"
            新坐标Y = 新坐标Y - 坐标增量
            If 确认坐标有效性(新坐标X, 新坐标Y) Then 
                Call 移动大地图(新坐标X, 新坐标Y)
                If 确认目标(寻找类型, 目标种类, 目标等级) Then 
                    目标坐标信息(0) = 新坐标X : 目标坐标信息(1) = 新坐标Y : 目标坐标信息(2) = i : 目标坐标信息(6) = d
                    WriteConfig "目标坐标信息"&账号信息&寻找类型, 目标坐标信息, True
                    Exit Sub
                End If
            End If
        Next
        d = 1
    Next
End Sub

Function 确认坐标有效性(坐标X, 坐标Y)
    If 坐标X < 0 Or 坐标Y < 0 Or 坐标X > 1000 Or 坐标Y > 1000 Then 
        确认坐标有效性 = False
        TracePrint "无效坐标 "& 坐标X & ","& 坐标Y
    Else 
        确认坐标有效性 = True
        TracePrint "有效坐标"&坐标X & "," & 坐标Y
    End If
End Function

Sub 移动大地图(坐标X, 坐标Y)
    容错参数 = 0
    Dim ocrchar1, ocrchar2, temp, temp1, 临时坐标X, 临时坐标Y
    Rem 确认X
    //输入X
    Touch 690, 620, 100
    Delay 100
    TracePrint "输入坐标"
    InputText 坐标X
    Delay 100
    //关闭输入框
    Touch 655, 650, 100
    Delay 100
    //点击搜索
    Touch 690, 800, 100
    Delay 1000
    ocrchar1 = 类型转换.SuperOcr(678, 597, 706, 648, "BFF3FB-111111|AFDCE2-111111|8FB0B3-111111|7F9A9C-111111|9FC6CB-111111", 0.9)
    临时坐标X = Int(ocrchar1)
    TracePrint 临时坐标X
    If 坐标X = 临时坐标X Then 
        TracePrint "X正确"
    Else 
        TracePrint "X错误"
        If 容错参数 < 2 Then
            容错参数 = 容错参数 + 1
            Goto 确认X
        End If
    End If
    
    容错参数 = 0
    Rem 确认Y
    //输入Y
    Touch 690, 730, 100
    Delay 100
    InputText 坐标Y
    Delay 100
    //关闭输入框
    Touch 655, 650, 100
    Delay 100
    //点击搜索
    Touch 690, 800, 100
    Delay 1000
    ocrchar2 = 类型转换.SuperOcr(678, 712, 706, 765, "BFF3FB-111111|AFDCE2-111111|8FB0B3-111111|7F9A9C-111111|9FC6CB-111111", 0.9)
    临时坐标Y = Int(ocrchar2)
    TracePrint 临时坐标Y
    If 坐标Y = 临时坐标Y Then 
        TracePrint "Y正确"
    Else 
        TracePrint "Y错误"
        If 容错参数 < 2 Then
            容错参数 = 容错参数 + 1
            Goto 确认Y
        End If
    End If
End Sub

Function 确认目标(寻找类型, 目标种类, 目标等级)
