                    容错参数 = 容错参数 + 1
                    Touch 150, 955, 100
                    Delay 1000
                    TracePrint "点击治疗,确认是否点击成功"
                    Goto 点击治疗	
                End If
                    
            End If
        Else 
            TracePrint "没伤兵啦"
        End If
    ElseIf 容错参数 < 5 Then
        容错参数 = 容错参数 + 1
        TracePrint "切换伤兵营失败,重试"
        Goto 切换到伤兵营
    End If
