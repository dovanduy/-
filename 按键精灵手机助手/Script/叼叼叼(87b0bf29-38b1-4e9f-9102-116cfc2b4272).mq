Sub 派遣队伍()
    //类型一: 1 = 普通刷怪, 2 = 皇榜任务
    ShowMessage "派遣队伍", 1000, 50, 620
    Dim 用户设置, 打怪队伍, 队伍状态, 伤兵信息, 伤兵比例, 是否使用行军令, 打怪种类, 打怪等级, 打怪次数, 满员率设置, 队伍兵量信息, 采集队伍, 采集类型, 矿坑等级, 智能采集, 寻访队伍, 是否自动筛选怪物, 寻访城池X坐标, 寻访城池Y坐标

    TracePrint "准备开始派遣队伍"

    用户设置 = 队伍设置()
    伤兵信息 = 兵事处理()
    队伍状态 = 判断队伍状态()
    打怪队伍 = 用户设置(0)
    采集队伍 = 用户设置(1)
    寻访队伍 = 用户设置(2)
    打怪种类 = 用户设置(3)
    打怪等级 = 用户设置(4)
    打怪次数 = 用户设置(5)
    是否使用行军令 = 用户设置(6)
    伤兵比例 = 用户设置(7)
    满员率设置 = 用户设置(8)
    采集类型 = 用户设置(9)
    矿坑等级 = 用户设置(10)
    智能采集 = 用户设置(11)
    是否自动筛选怪物 = 用户设置(12)
    寻访城池X坐标 = 用户设置(13)
    寻访城池Y坐标 = 用户设置(14)
    For i = 0 To 3
        If 打怪队伍(i) = True Then 
            If 队伍状态(i) = 1 Then 
                TracePrint "判断伤兵比例", (伤兵信息(0) / 伤兵信息(1) * 100)
                If (伤兵信息(0) / 伤兵信息(1) * 100) < 伤兵比例 Then 
                    队伍兵量信息 = 判断队伍兵量(i)
                    If 队伍兵量信息(3) > 满员率设置 Then 
                        TracePrint 打怪次数, 派遣出征次数
                        If 打怪次数 > 派遣出征次数 Then 
                            TracePrint "出城寻找怪物"
                            Call 搜索目标(1, 打怪种类, 打怪等级, 是否自动筛选怪物)
                            Call 前往目标(1, i)
                        End If
                    End If
                End If
            End If	
        End If
    Next
    队伍状态 = 判断队伍状态()
    For i = 0 To 3
        TracePrint "采集队伍"&i,采集队伍(i)
        If 采集队伍(i) = True Then 
            TracePrint "队伍状态"&i,队伍状态(i)
            If 队伍状态(i) = 1 Then 
                队伍兵量信息 = 判断队伍兵量(i)
                If 队伍兵量信息(3) > 满员率设置 Then 
                    If 智能采集 = True Then 
                        采集类型 = Array()
                        采集类型(最少资源类型()) = True
                        For Each a In 采集类型
                            TracePrint a
                        Next
                    End If
                    TracePrint "出城寻找资源"
                    Call 搜索目标(2, 采集类型, 矿坑等级, True)
                    Call 前往目标(2, i)
                End If
            End If	
        End If
    Next
    队伍状态 = 判断队伍状态()
    For i = 0 To 3
        TracePrint "寻访队伍"&i,寻访队伍(i)
        If 寻访队伍(i) = True Then 
            TracePrint "队伍状态"&i,队伍状态(i)
            If 队伍状态(i) = 1 Then 
                TracePrint "出城寻访"
                Call 去野外(1)
                Call 界面收起展开(3)
                Call 移动大地图(寻访城池X坐标, 寻访城池Y坐标)
                
                容错参数 = 0
                Rem 点击城池
                Touch 380, 640, 100
                Delay 1000
                Dim intX,intY
                FindPic 185,666,617,1019,"Attachment:寻访图标.png","000000",0,0.7,intX,intY
                If intX > -1 And intY > -1 Then
                    TracePrint "点击城池成功"
                    容错参数 = 0
                    Rem 点击寻访图标
                    Touch intX + 5, intY + 5, 100
                    Delay 1000
                    所在场景 = 场景相关操作.所在场景()
                    If 所在场景 = 48 Then 
                        TracePrint "进入寻访界面"
                        容错参数 = 0
                        Rem 识别剩余次数
                        Dim ocrchar
                        ocrchar=Ocr(493,578,517,619,"489867-111111|5BC182-111111|3E7F57-111111",0.9)
                        Traceprint ocrchar
                        If Len(ocrchar) = 3 Then 
                            If Int(Left(ocrchar, 1)) = 0 Then 
                                TracePrint "今日寻访次数已用完"
                            Else 
                                Call 前往目标(3, i)
                            End If
                        ElseIf 容错参数 < 5 Then
                            容错参数 = 容错参数 + 1
                            TracePrint "识别错误,重试"
                            Goto 识别剩余次数
                        End If
                    ElseIf 容错参数 < 5 Then
                        TracePrint "进入寻访界面失败,重试"
                        容错参数 = 容错参数 + 1
                        Goto 点击寻访图标
                    End If
                ElseIf 容错参数 < 5 Then
                    容错参数 = 容错参数 + 1
                    TracePrint "点击城池失败,重试"
                    Goto 点击城池
                End If
            End If	
        End If
    Next
End Sub

