                        TracePrint "即将退出升星"
                        Exit Do
                    End If
                Loop
            End If
            Rem 关闭界面
            Touch 680, 1230, 100
            Delay 1500
            If Not 场景相关操作.进入场景是否正确(14) Then 
                Goto 关闭界面
            End If
        Else 
            Exit Do
        End If
    Loop
End Sub


Sub 强化前几个人( 类型)
    //暂时最多支持9人
    //类型枚举 : 1 = 装备 2 = 升星 3 = 升阶 4 = 升级
    TracePrint "开始强化武将,类型为"&类型
    Dim 人数 = ReadUIConfig("强化人数", 6)
    Select Case 类型
    Case 1
        If ReadUIConfig("强化装备") = False Then 
            TracePrint "设置不自动强化装备"
            Exit Sub
        End If
    End Select
    
    //进武将界面
    进出界面.城内进固定界面(14, 相关数据解析.武将坐标X, 相关数据解析.武将坐标Y, 5, False)
    Dim 武将起始坐标X, 武将起始坐标Y, X偏移量, Y偏移量, 当前武将坐标X, 当前武将坐标Y
    武将起始坐标X = 485 : 武将起始坐标Y = 300 : X偏移量 = 150 : Y偏移量 = 340
    For i = 0 To (人数 - 1)
        当前武将坐标X = 武将起始坐标X - Int(i / 3) * X偏移量
        当前武将坐标Y = 武将起始坐标Y + (i Mod 3) * Y偏移量
        Touch 当前武将坐标X, 当前武将坐标Y, 100
        Delay 1500
        Select Case 类型
        Case 1
            Call 强化单个武将装备()
            Delay 1500
        Case 2
        Case 3
            Call 单个武将升阶()
            If Not 体力判断() Then 
                Exit Sub
            End If
        Case 4
        End Select   
    Next
    TracePrint "强化武将完毕"
    场景相关操作.通用界面关闭 
    Delay 2000
End Sub

Sub 强化单个武将装备()
    Dim 装备坐标X, 装备坐标Y, 偏移量, intX, intY
    容错参数 = 0
    Rem 进强化界面
    TracePrint "选择装备"
    Touch 125, 660, 100
    Delay 500
    TracePrint "进强化界面"
    Touch 125 + 140, 660 + 55, 100
    Delay 1000
    FindPic 80,911,159,1143,"Attachment:一键强化.png","000000",0,0.9,intX,intY
    If intX > -1 And intY > -1 Then 
        TracePrint "进入强化界面"
    Else 
        FindPic 80,911,159,1143,"Attachment:不可一键强化.png","000000",0,0.9,intX,intY
        If intX > -1 And intY > -1 Then
            TracePrint "等级或者铜币不足不能强化"
            Exit Sub
        Else 
            FindPic 137,785,214,981,"Attachment:满级.png","000000",4,0.9,intX,intY
            If intX > -1 And intY > -1 Then
                TracePrint "满级不能强化"
                Exit Sub
            ElseIf 容错参数 < 3 Then
                TracePrint "进强化界面出错,重试"
                容错参数 = 容错参数 + 1
                Goto 进强化界面
            ElseIf 容错参数 >= 3 Then
                TracePrint "错误次数太多,放弃"
                Exit Sub
            End If
        End If
    End If
    装备坐标X = 660 : 装备坐标Y = 200 : 偏移量 = 125
    For 4
        装备坐标X = 装备坐标X - 偏移量
        //选择装备
        Touch 装备坐标X, 装备坐标Y, 100
        Delay 1000
        容错参数 = 0
        Rem 点击一键强化
        Touch 120, 1025, 100
        Delay 1000
        FindPic 80,911,159,1143,"Attachment:一键强化.png","000000",0,0.9,intX,intY
        If intX > -1 And intY > -1 And 容错参数 < 3 Then 
            容错参数 = 容错参数 + 1
            TracePrint "点击一键强化失败,重试"
            Goto 点击一键强化
        Else 
            FindPic 80,911,159,1143,"Attachment:不可一键强化.png","000000",0,0.9,intX,intY
            If intX > -1 And intY > -1 Then
                TracePrint "等级或者铜币不足不能强化"
            Else 
                FindPic 137,785,214,981,"Attachment:满级.png","000000",4,0.9,intX,intY
                If intX > -1 And intY > -1 Then
                    TracePrint "满级不能强化"
                ElseIf 容错参数 < 3 Then 
                    TracePrint "异常状况"
                    容错参数 = 容错参数 + 1
                    场景相关操作.通用界面关闭
                    Goto 点击一键强化
                ElseIf 容错参数 >= 3 Then
                    TracePrint "错误次数过多,放弃"
                    Exit For
                End If
            End If
        End If
    Next
    TracePrint "强化单个武将装备完成"
    //关闭强化界面
    场景相关操作.通用界面关闭
    Delay 1500
    //关闭武将详情界面
    场景相关操作.通用界面关闭 
    Delay 1500
End Sub

