Sub 回主城(类型)
    //回主城类型:1 = 不关心初始位置, 2 = 必须回到登录时的初始位置
    Select Case 类型
    Case 1
        容错参数 = 0
        Rem 回主城1
        Delay 2000
        所在场景 = 场景相关操作.所在场景()
        If 所在场景 <> 1 And 容错参数 < 5 Then
            容错参数 = 容错参数 + 1
            场景相关操作.关闭界面
            Goto 回主城1
        End If
    Case 2
        容错参数 = 0
        Rem 回主城2
        Delay 2000
        所在场景 = 场景相关操作.所在场景()

        If 所在场景 = 1 Then
            Dim intX,intY
            FindPic 219,263,321,407,"Attachment:中间城墙贴图1.png","000000",0,0.9,intX,intY
            If intX > -1 And intY > -1 Then
                TracePrint "在主城初始位置"
            Else 
                FindPic 264,917,311,1057,"Attachment:中间城墙贴图2.png","000000",0,0.96,intX,intY
                If intX > -1 And intY > -1 Then 
                    TracePrint "在主城初始位置"
                Else 
                    If 容错参数 < 5 Then
                        容错参数 = 容错参数 + 1
                        Touch 地图坐标X, 地图坐标Y, 100
                        Goto 回主城2
                    End If
                End If
            End If
        Else 
            If 容错参数 < 5 Then
                容错参数 = 容错参数 + 1
                场景相关操作.关闭界面 
                Goto 回主城2
            End If
        End If
    End Select
    TracePrint "当前在主城中"
End Sub

Sub 去野外(类型)
    //去野外类型:1 = 不关心初始位置, 2 = 必须回到初始位置
    Select Case 类型
    Case 1
        容错参数 = 0
        Rem 去野外1
        所在场景 = 场景相关操作.所在场景()
        If 所在场景 = 1 Then 
            Touch 地图坐标X, 地图坐标Y, 100
            Delay 3000
            Goto 去野外1
        ElseIf 所在场景 = 2 Then
            TracePrint "在野外啦!"
        Else 
            If 容错参数 < 5 Then 
                容错参数 = 容错参数 + 1
                场景相关操作.关闭界面 
                Delay 1000
                Goto 去野外1
            End If
        End If
    Case 2
        容错参数 = 0
        Rem 去野外2
        所在场景 = 场景相关操作.所在场景()
        If 所在场景 = 1 Then 
            Touch 地图坐标X, 地图坐标Y, 100
            Delay 3000
            Goto 去野外2
        ElseIf 所在场景 = 2 Then
            移动大地图(Int(野外主城坐标X), Int(野外主城坐标Y))
        Else 
            If 容错参数 < 8 Then 
                容错参数 = 容错参数 + 1
                场景相关操作.关闭界面 
                Delay 1000
                Goto 去野外2
            End If
        End If
    End Select
End Sub

Sub 界面收起展开(类型)
    //类型枚举 1 = 主城顶部收起, 2 = 主城底部收起, 3 = 野外顶部收起, 4 = 主城顶部展开, 5 = 主城底部展开, 6 = 野外顶部展开
    Select Case 类型
    Case 1, 4
        容错参数 = 0
        Rem 主城顶部操作
        Delay 2000
        所在场景 = 场景相关操作.所在场景()
        If 所在场景 = 1 And 容错参数 < 8 Then
            If 场景相关操作.主城顶部是否收起() = False Then 
                If 类型 = 1 Then 
                    容错参数 = 容错参数 + 1
                    Touch 顶部收起X, 顶部收起Y, 100
                    TracePrint "点了主城顶部收起坐标是",顶部收起X, 顶部收起Y
                    Goto 主城顶部操作
                End If
            Else
                If 类型 = 4 Then 
                    容错参数 = 容错参数 + 1
                    Touch 顶部收起X, 顶部收起Y, 100
                    TracePrint "点了主城顶部展开坐标是",顶部收起X, 顶部收起Y
                    Goto 主城顶部操作 
                End If
            End If
        ElseIf 容错参数 < 8 Then
            容错参数 = 容错参数 + 1
            回主城(1)
        End If
    Case 2, 5
        容错参数 = 0
        Rem 底部操作
        Delay 2000
        所在场景 = 场景相关操作.所在场景()
        If (所在场景 = 1 Or 所在场景 = 2) And 容错参数 < 8 Then 
            If 场景相关操作.主城底部是否收起() = False Then 
                If 类型 = 2 And 容错参数 = 0 Then 
                    容错参数 = 容错参数 + 1
                    Touch 收起坐标X, 收起坐标Y, 50
                    TracePrint "点了主城底部收起坐标是",收起坐标X, 收起坐标Y
                    Goto 底部操作
                End If
            Else 
                If 类型 = 5 Then 
                    容错参数 = 容错参数 + 1
                    Touch 展开坐标X, 展开坐标Y, 50
                    TracePrint "点了主城底部展开坐标是",展开坐标X, 展开坐标Y
                    Goto 底部操作
                End If
            End If
        ElseIf 容错参数 < 8 Then
            容错参数 = 容错参数 + 1
            场景相关操作.关闭界面
        End If
    Case 3, 6
        容错参数 = 0
        Rem 野外顶部操作
        If 场景相关操作.野外顶部是否收起() = False Then 
            If 类型 = 3 And 容错参数 < 8 Then 
                容错参数 = 容错参数 + 1
                Touch 600, 1140, 50
                TracePrint "点了野外顶部收起坐标是",收起坐标X, 收起坐标Y
                Goto 野外顶部操作
            End If
        Else 
            If 类型 = 6 And 容错参数 < 8 Then 
                容错参数 = 容错参数 + 1
                Touch 600, 1140, 50
                TracePrint "点了野外顶部展开坐标是",展开坐标X, 展开坐标Y
                Goto 野外顶部操作
            End If
        End If
    End Select
End Sub

Sub 定位主城坐标()
    ShowMessage "定位主城坐标", 1000, 50, 620
    野外主城坐标X = Int(ReadUIConfig("主城X坐标")) : 野外主城坐标Y = Int(ReadUIConfig("主城Y坐标"))
    If 野外主城坐标X <> 0 And 野外主城坐标Y <> 0 Then 
        TracePrint "用户自己设置了主城坐标,不用定位了"
        Exit Sub 
    End If
    Call 回主城(1)
    Call 去野外(1)
    UseDict (1)
    容错参数 = 0
    Rem 定位坐标
    Dim ocrchar, temp, temp1, tempStr
    ocrchar=Ocr(669,515,720,784,"B3C6C8-111111|D5EEF2-111111|BDF1F8-111111|ACD8DE-111111|000000-111111",0.9)
    Traceprint ocrchar
    temp = InStr(1, ocrchar, "Y")
    temp1 = Len(ocrchar)
    野外主城坐标X = Int(Mid(ocrchar, 3, temp - 3))
    野外主城坐标Y = Int(Right(ocrchar, temp1 - temp - 1))
    tempStr = "主城坐标" & 野外主城坐标X & "," & 野外主城坐标Y
    TracePrint tempStr
    If (野外主城坐标X = 0 Or 野外主城坐标Y = 0) And 容错参数 < 5 Then 
        容错参数 = 容错参数 + 1
        TracePrint "坐标定位有问题,重试"
        Goto 定位坐标
    End If
    ShowMessage tempStr, 1000, 50, 620
    ShowMessage "如果主城坐标识别不正确,请停止运行脚本,在设置中手动输入", 1000, 50, 620
    Delay 1000
End Sub


Sub 小红点处理()
    TracePrint "开始小红点处理"
    ShowMessage "小红点处理", 1000, 50, 620
    Call 回主城(1)
    Call 界面收起展开(4)
    Call 界面收起展开(5)
    Delay 1500
    Dim 小红点列表 = 小红点.小红点列表()
    //小红点枚举
    //1 = 邮件, 2 = 任务, 3 = 活动, 4 = 七日狂欢, 5 = 开服基金 , 6 = 月卡 , 7 = 皇榜任务, 8 = 签到, 9 = 副本, 10 = 军团, 11 = 背包, 12 = 武将, 13 = 沙场论剑, 14 = 兵事 ,15 = 俸禄
    //16 = 
    For Each a In 小红点列表
        TracePrint "小红点类型",a(0), "小红点状态",a(1)
        If a(1) = True Then 
            Touch a(2) - 5, a(3) - 5, 100
            Delay 1000
            If a(0) = 1 Then
                Call 收邮件()
            ElseIf a(0) = 2 Then
                Call 领任务奖励()
            ElseIf a(0) = 3 Then
                Call 领取体力()
            ElseIf a(0) = 4 Then
            ElseIf a(0) = 5 Then
            ElseIf a(0) = 6 Then
            ElseIf a(0) = 7 Then
                Call 进皇榜界面()
                Call 领取皇榜任务奖励()
            ElseIf a(0) = 8 Then
                Call 签到()
            ElseIf a(0) = 9 Then
                Call 进副本(1)
                Call 领取星级奖励()
                Call 进副本(2)
                Call 领取星级奖励()
                If ReadUIConfig("是否打名将副本") = True Then 
                    Call 副本准备(3, 1, 6)
                End If
            ElseIf a(0) = 10 Then
                Call 军团小红点处理()
            ElseIf a(0) = 11 Then
                Call 背包小红点处理()
            ElseIf a(0) = 12 Then
                Call 武将小红点处理()
            ElseIf a(0) = 13 Then
            ElseIf a(0) = 14 Then
                Call 兵事处理()
            ElseIf a(0) = 15 Then
                Call 领俸禄()
            End If
        End If
    Next
    TracePrint "小红点处理完毕"
End Sub

Sub 领俸禄()
