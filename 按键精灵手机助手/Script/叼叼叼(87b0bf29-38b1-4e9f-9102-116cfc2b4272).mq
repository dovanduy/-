Sub 获取单个副本数据(副本数据)
    容错参数 = 0
    Rem 数据识别
    Dim ocrchar, 字符长度, intX,intY
    副本数据(3) = 0
    FindPic 106,846,220,1052,"Attachment:重置.png","000000",0,0.9,intX,intY
    If intX > -1 And intY > -1 Then 
        副本数据(3) = 0
    Else 
        ocrchar = 类型转换.SuperOcr(459,864,483,934,"B3C7D1-111111|7B868B-111111|8B989E-111111|91A0A8-111111|A0B1BA-111111",1)
        Traceprint ocrchar
        字符长度 = UTF8.Len(ocrchar)
        If 字符长度 = 2 Or 字符长度 = 3 Then  
            副本数据(3) = Int(UTF8.Left(ocrchar, 1))
