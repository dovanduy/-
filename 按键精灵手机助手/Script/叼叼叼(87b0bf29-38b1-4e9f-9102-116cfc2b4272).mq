Sub 祭祀等待(类型)
    Rem 识别文字
    Delay 1000
    Dim ocrchar 
    ocrchar = 类型转换.SuperOcr(373,956,535,1017,"F4FFFF-111111|FDFFFF-111111|D6DEEA-111111",1)
    TracePrint ocrchar
    祭祀切换 = False
    Select Case 类型
    Case 1
        If UTF8.InStr(1, ocrchar, "农") > 0 Then
            祭祀切换 = True
            Exit Function
        End If
    Case 2
        If UTF8.InStr(1, ocrchar, "木") > 0 Then 
            祭祀切换 = True
            Exit Function
        End If
    Case 3
        If UTF8.InStr(1, ocrchar, "石") > 0 Then 
            祭祀切换 = True
            Exit Function
        End If
    Case 4
        If UTF8.InStr(1, ocrchar, "矿") > 0 Then 
            祭祀切换 = True
            Exit Function
        End If
    End Select
    祭祀切换 = False
End Sub

Function 祭祀次数()	
    Dim temp1, 今日已祭祀次数
    容错参数 = 0
    Rem 开始识别文字
    Dim ocrchar
    TracePrint "开始识别文字"
    ocrchar = 类型转换.SuperOcr(184,697,214,786,"FFFFFF-111111|C7CCD4-111111|EDF4FD-111111|E6EDF5-111111|C9CFD7-111111|B2B7BD-111111|919191-111111|AAA6A6-111111|E1E1E1-111111",0.9)
    TracePrint ocrchar	
    If ocrchar = "" And 容错参数 < 3 Then 
        容错参数 = 容错参数 + 1
        Goto 开始识别文字
    ElseIf 容错参数 >= 3 Then
        ocrchar = "0/99"
    End If
    temp1 = 类型转换.字符串解析带符号(ocrchar, "/", 3, 5)
    今日已祭祀次数 = temp1(1) - temp1(0)
    TracePrint "今日已祭祀次数"&今日已祭祀次数&"祭祀次数上限"&temp1(1)&"剩余祭祀次数"&temp1(0)
    祭祀次数 = 今日已祭祀次数
End Function

