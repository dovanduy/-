Sub 启动游戏()
    If 平台 = 0 Then 
        If Sys.AppIsFront("com.game.sgz.a360") Then 
            TracePrint "当前游戏已经启动"
        Else 
            TracePrint "准备启动游戏"
            RunApp "com.game.sgz.a360"
            Delay 5000
        End If
    ElseIf 平台 = 1 Then
        If Sys.AppIsFront("com.game.sgz.a360.yod") Then 
            TracePrint "当前游戏已经启动"
        Else 
            TracePrint "准备启动游戏"
            RunApp "com.game.sgz.a360.yod"
            Delay 5000
        End If
    ElseIf 平台 = 2 Then
        If Sys.AppIsFront("com.shouyou.sgzerq.hm") Then 
            TracePrint "当前游戏已经启动"
        Else 
            TracePrint "准备启动游戏"
            RunApp "com.shouyou.sgzerq.hm"
            Delay 5000
        End If
    ElseIf 平台 = 3 Then
        If Sys.AppIsFront("com.kkl.xsg2018.tf") Then 
            TracePrint "当前游戏已经启动"
        Else 
            TracePrint "准备启动游戏"
            RunApp "com.kkl.xsg2018.tf"
            Delay 5000
        End If
    End If
End Sub

Sub 关闭游戏()
    If 平台 = 0 Then 
        KillApp "com.game.sgz.a360"
    ElseIf 平台 = 1 Then
        KillApp "com.game.sgz.a360.yod"
    ElseIf 平台 = 2 Then
        KillApp "com.shouyou.sgzerq.hm"
    ElseIf 平台 = 3 Then
        KillApp "com.kkl.xsg2018.tf"
    End If
    Delay 5000
End Sub

Sub 初始化杂项配置
    //读取上次运行时间,决定数据是否要更新
    Dim 上次运行日期, 时间差, 本次运行日期, temp1, temp2
    
    上次运行日期 = ReadConfig("上次运行日期", 0)
    TracePrint "上次运行日期"&上次运行日期
    ShowMessage "上次运行日期"&上次运行日期, 1000, 50, 620
    本次运行日期 = Date()
    TracePrint 本次运行日期
    temp1 = Int(Right(上次运行日期, 2))
    temp2 = Int(Right(本次运行日期, 2))
    
    时间差 = temp1 - temp2
    If ReadUIConfig("是否清理数据") Or 时间差 <> 0 Then 
        //重置祭祀数据
        WriteConfig "是否已经祭祀"&账号信息, False, True

        //重置推关状态
