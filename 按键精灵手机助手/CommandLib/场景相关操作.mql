    TracePrint "区服",区服
    容错次数 = ReadConfig("选服重试次数", 3)
    TracePrint "容错次数",容错次数
    //先判断当前默认区服是否和填写的区服一致
    ocrchar=类型转换.SuperOcr(233,588,260,670,"BDBDBC-111111|DBDBDB-111111|A6A9A9-111111",0.9)
    If ocrchar = 区服 Then 
        TracePrint "选择一致,完成"
        Exit Function
    End If
    //进选服界面
